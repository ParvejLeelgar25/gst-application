  <action-view name="gst.root.party.party" title="Party" model="com.axelor.gst.db.Party">
  	<view type="grid" name="party-grid"/>
  	<view type="form" name="party-form"/>
  	<view type="cards" name="party-card"/>
  	<context name="json-enhance" expr="true"/>
  </action-view>
  
  <menuitem title="Contact" name="gst-root-party-contact" parent="gst-root-party" action="gst.root.party.contact"/>
  
