  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
  
  <menuitem title="GST" name="gst-root"/>
  
  <menuitem title="Dashboard" name="gst-root-dashboard" parent="gst-root"/>
  
  <menuitem title="Dashboard" name="gst-root-dashboard-dashboard" parent="gst-root-dashboard" action="gst.root.dashboard.dashboard"/>
  
  <action-view name="gst.root.dashboard.dashboard" title="DashBoard" model="">
  	<view type=""/>
