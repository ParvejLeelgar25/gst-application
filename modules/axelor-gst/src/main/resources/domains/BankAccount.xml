<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="gst" package="com.axelor.gst.db" />
	
	<entity name="BankAccount">
		<string name="bank" required="true" title="Bank"/>
		<string name="accountNo" required="true" title="Account"/>
		<string name="ifsc" required="true" title="IFSC"/>
		<string name="type" selection="bankAccount.type.selection" title="Type" default="current"/>
		<string name="address" large="true" title="Address"/>
		
		<many-to-one name="company" ref="com.axelor.gst.db.Company"/>
	</entity>
	
	</domain-models>