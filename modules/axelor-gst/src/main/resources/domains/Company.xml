<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="gst" package="com.axelor.gst.db" />

	<entity name="Company">
		<string name="name" required="true" title="Name"/>
		<string name="gstIn" min="15" max="15" title="GSTIN"/>
		<one-to-many name="contactList" ref="com.axelor.gst.db.Contact" title="Contact"/>  <!-- mappedBy="company" -->
		<one-to-many name="bankDetailsList" ref="com.axelor.gst.db.BankAccount" title="Bank Account Details"/> <!-- mappedBy="company" -->
		<many-to-one name="logo" ref="com.axelor.meta.db.MetaFile" title="Image"/>
		<many-to-one name="address" ref="com.axelor.gst.db.Address" title="Address" />
		
		<!-- <one-to-many name="invoiceList" ref="com.axelor.gst.db.Invoice" mappedBy="company"/> -->
	</entity>
</domain-models>